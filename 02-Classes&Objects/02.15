public class Lamp
{
    boolean On= false;
    
    public void SwitchOn()
    {   
        On = true;
    }
    public void SwitchOff()
    {   
        On= false;
    }
    public void State()
    {   if(On==true)
        {System.out.println("Lamp is on.");}
        
        if(On==false)
        {System.out.println("Lamp is off.");}
    }
    public static void main (String[] args)
    {
        Lamp lamp1= new Lamp();
        Lamp lamp2= new Lamp();
        lamp1.SwitchOn();
        lamp1.State();
        lamp2.State();
    }
}